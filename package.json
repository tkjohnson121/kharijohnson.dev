    "@types/react-dom": "^16.9.4",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.3.1",
    "typescript": "4.0"
  },
  "license": "MIT"
}
